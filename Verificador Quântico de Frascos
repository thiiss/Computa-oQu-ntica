        "# Configura√ß√£o do simulador\n",
        "simulator = Aer.get_backend('aer_simulator')\n",
        "\n",
        "# Execu√ß√£o com 1000 shots (execu√ß√µes)\n",
        "result = execute(qc, simulator, shots=1000).result()\n",
        "counts = result.get_counts()\n",
        "\n",
        "# Visualiza√ß√£o dos resultados\n",
        "print(\"\\nResultados da medi√ß√£o:\")\n",
        "print(counts)\n",
        "plot_histogram(counts, title=\"Resultados do Controle de Qualidade\", color=['#1f77b4', '#ff7f0e', '#2ca02c', '#d62728'])\n",
        "\n",
        "# Interpreta√ß√£o detalhada\n",
        "print(\"\\nüìä Interpreta√ß√£o:\")\n",
        "print(f\"00: Embalagem fora do padr√£o e KPI ruim ({counts.get('00', 0)} ocorr√™ncias)\")\n",
        "print(f\"01: INCONSIST√äNCIA (embalagem ruim mas KPI bom) ({counts.get('01', 0)} ocorr√™ncias)\")\n",
        "print(f\"10: Embalagem padr√£o mas KPI n√£o afetado ({counts.get('10', 0)} ocorr√™ncias)\")\n",
        "print(f\"11: Embalagem padr√£o e KPI bom ({counts.get('11', 0)} ocorr√™ncias)\")\n",
        "\n",
        "# An√°lise de qualidade\n",
        "if counts.get('01', 0) > 50:  # Mais de 5% de inconsist√™ncias\n",
        "    print(\"\\n‚ö†Ô∏è ALERTA: Processo com alta taxa de inconsist√™ncias!\")\n",
        "else:\n",
        "    print(\"\\n‚úÖ Processo dentro dos par√¢metros aceit√°veis\")"
      ],
